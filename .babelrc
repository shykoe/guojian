{
    "presets": ["es2015", "react", "stage-0"],
    "plugins": [
      "transform-react-jsx",
      "add-module-exports",
        ["babel-plugin-transform-builtin-extend", {
            "globals": ["Error"]
        }],
        ["transform-runtime", {
            "polyfill": false,
            "regenerator": true
        }]
    ]
}
